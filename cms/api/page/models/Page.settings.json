{
  "connection": "default",
  "collectionName": "homepages",
  "info": {
    "name": "page",
    "description": "Velden voor uw pagina's"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "twallieDescription": {
      "maxLength": 200,
      "default": "",
      "unique": true,
      "minLength": 10,
      "type": "string"
    },
    "pageBackground": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "type": {
      "unique": true,
      "required": true,
      "type": "string"
    }
  }
}